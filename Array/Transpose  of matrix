#include<iostream>
using namespace std;
int arr[][3]={1,2,3,4,5,6,7,8,9};
int i,j;
int main(){
for(i=0;i<=2;i++){
    for(j=0;j<=2;j++){
        cout<<arr[i][j]<<" ";
    }
    cout<<endl;
}
cout<<endl;
for(i=0;i<=2;i++){
    for(j=0;j<=2;j++){
        cout<<arr[j][i]<<" ";
    }
    cout<<endl;
}
}
