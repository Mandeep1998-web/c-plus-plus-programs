#include<iostream>
using namespace std;
int main(){
    int i,j,sum=0;
int arr[3][3]={1,2,3,1,5,6,1,1,9};
for(i=0;i<3;i++){
    for(j=0;j<3;j++){
        cout<<arr[i][j]<<" ";
    }
    cout<<endl;
}
for(i=0;i<2;i++){
    for(j=1+i;j<=2;j++){
            sum=sum+arr[j][i];

    }
}
cout<<"the sum is "<<sum;

}
