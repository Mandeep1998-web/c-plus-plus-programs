// program to rotate right the elements of the array
#include<iostream>
using namespace std;

int rotaterightonce(int arr[], int n ){
int i;
int temp=arr[0];
for (i=0;i<n;i++)
{
    arr[i]=arr[i+1];
}
arr[n-1]=temp;
}


int rotateright(int arr[],int l ,int d){
    int i;
for (i=0;i<d;i++){
    rotaterightonce(arr,l);
}
for (i=0;i<l;i++)
{
 cout<<arr[i];

}
}
int main()
{
    int n;

int arr[]={1,2,3,4,5};
n=sizeof(arr)/sizeof(arr[0]);
int k=3;
 rotateright(arr,n,k);

}
